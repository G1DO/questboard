{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "web",
      "image": "g1do/questboard-web:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    },
    {
      "name": "db",
      "image": "postgres:16",
      "essential": false,
      "environment": [
        { "name": "POSTGRES_USER", "value": "questdb" },
        { "name": "POSTGRES_PASSWORD", "value": "questdb" },
        { "name": "POSTGRES_DB", "value": "questdb" }
      ],
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 5432,
          "hostPort": 5432
        }
      ]
    }
  ]
}
